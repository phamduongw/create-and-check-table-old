    "connection.user": "T24SB",
    "connection.password": "xxxx",
    "dialect.name": "OracleDatabaseDialect",
    "connection.attempts": "10",
    "connection.backoff.ms": "10000",
    "insert.mode": "UPSERT",
    "table.types": "TABLE",
    "table.name.format": "T24SB.ODS_{CONNECTOR_NAME}",
    "pk.mode": "record_value",
    "pk.fields": "RECID",
    "auto.create": "false",
    "auto.evolve": "false",
    "delete.mode": "UPDATE",
    "delete.as.update.identifier": "COMMIT_ACTION=D",
