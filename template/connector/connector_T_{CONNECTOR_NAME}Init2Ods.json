    "delete.as.update.key": "RECID",
    "audit.timestamp.column": "TIME_UPDATE",
    "principal.service.name": "kafka_connect_out",
    "principal.service.password": "xxx",
    "value.converter.schema.registry.url": "http://dc2-oscope-kkschm01:8081,http://dc2-oscope-kkschm02:8081"
  }
